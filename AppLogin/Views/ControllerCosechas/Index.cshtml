@model IEnumerable<AppLogin.Model.Cosechas>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaCosecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CantidadRecogida)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCultivo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTemporada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Eliminado)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCosecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CantidadRecogida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCultivo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTemporada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Eliminado)
            </td>
            <td>
                    <a asp-action="Edit" class="btn btn-warning" asp-route-id="@item.IdCosechas">Edit</a> |
                    <a asp-action="Details" class="btn btn-primary" asp-route-id="@item.IdCosechas">Details</a> |
                    
            </td>
        </tr>
}
    </tbody>
</table>
